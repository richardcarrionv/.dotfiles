alt + shift + Tab
  i3-display-swap.sh

alt + control + space
  toggle-keyboard-layout

alt + Return
  rof-scratch-term

alt + shift + Return
  ros-mainterm

alt + control + Return
  rof-quickedit

alt + comma 
  i3-rofi-scratchpad

alt + c 
  rof-qalc

alt + control + 0
  rof-color-picker

alt + control + p 
  rof-spotify

alt + control + f 
  rof-front

alt + e ; {c}
	{rof-charles}

alt + control + i
  rof-linear

alt + control + s
  rof-slack

alt + t 
  rofi -show tr -modes 'tr:rofi-trans.sh' -m normal

alt + shift + t
  deepl

alt + control + t
  alacritty --class term --title Terminal --command zsh

alt + w
  ~/bin/zen.AppImage

alt + shift + w; {w,s}
  {i3-msg workspace 2 && brave-browser, brave-browser --new-window https://web.whatsapp.com}

alt + x 
	rofi-main

alt + control + n 
	rof-notes

alt + control + z 
	polybar-msg cmd toggle

XF86AudioRaiseVolume  
  pactl set-sink-volume @DEFAULT_SINK@ +5% && $refresh_i3status

XF86AudioLowerVolume  
  pactl set-sink-volume @DEFAULT_SINK@ -5% && $refresh_i3status

XF86AudioMute         
  pactl set-sink-mute @DEFAULT_SINK@ toggle && $refresh_i3status

XF86AudioMicMute      
  pactl set-source-mute @DEFAULT_SOURCE@ toggle && $refresh_i3status

XF86MonBrightnessUp   
  brightnessctl s +2%

XF86MonBrightnessDown 
  brightnessctl s 2%-

Print                 
  flameshot gui

